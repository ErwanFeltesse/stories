<Section class= "formular" style="margin: 100px;">
<h1>Sign Up</h1>

<div class="form-container">
  <%= form_with model: @signup, local: true do |form| %>
    <div>
      <%= form.label :firstname %><br>
      <%= form.text_field :firstname %>
    </div>

    <div>
      <%= form.label :email %><br>
      <%= form.email_field :email %>
    </div>

    <div>
      <%= form.submit "Sign Up" %>
    </div>
  <% end %>
</div>
</ Section>
